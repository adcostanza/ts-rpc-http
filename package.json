{
  "name": "ts-rpc-http",
  "version": "0.0.37",
  "description": "Quickly generate a typesafe RPC API over HTTP with Express along with a typesafe rxjs client",
  "main": "index.js",
  "scripts": {
    "build": "rm -Rf dist && tsc && cp src/template.handlebars dist/",
    "package": "npm run build && cp package.json dist && cp README.md dist && cp -R bin dist && cd dist && npm pack",
    "publish": "npm run package && cd dist && npm publish ts-rpc-http*.tgz",
    "generateTestClients": "ts-node src/cli.ts --model test/models.ts --clients clients.ts",
    "generateJsonSchemas": "ts-node src/cli.ts --model test/models.ts --schemas",
    "coveralls": "jest test --coverage && cat ./coverage/lcov.info | coveralls",
    "test": "jest test"
  },
  "bin": {
    "ts-generate": "./bin/generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^10.12.21",
    "coveralls": "^3.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.5",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "handlebars": "^4.0.12",
    "jest": "^24.1.0",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "prettier": "^1.16.4",
    "typescript-json-schema": "^0.35.0",
    "xmlhttprequest": "^1.8.0"
  }
}
